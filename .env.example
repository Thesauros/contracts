# Deployer private key (WITH 0x prefix)
DEPLOYER_PRIVATE_KEY=0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa

# Alchemy project ID
ALCHEMY_PROJECT_ID=your-alchemy-project-id

# Etherscan API key (V2 multichain; used for contract verification on Arbitrum)
ETHERSCAN_API_KEY=your-etherscan-api-key

# Protocol treasury address
TREASURY_ADDRESS=0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa

# Full RPC URL for testing
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your_project_id

# Withdraw fee percent in wad (1e18 = 100%, max 0.05 * 1e18 = 5%)
# Default: 0
WITHDRAW_FEE_PERCENT=

# Timelock delay in seconds (min 1800 = 30 minutes, max 2592000 = 30 days)
# Default: 1800
TIMELOCK_DELAY=